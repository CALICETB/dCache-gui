# dCache-guiConfig.cmake.in
#
# cmake config file for DCACHE_GUI
#  DCACHE_GUI_INCLUDE_DIRS  - the DCACHE_GUI include directory
#  DCACHE_GUI_LIBRARIES     - The libraries needed to use DCACHE_GUI
#  DCACHE_GUI_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( DCACHE_GUI_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@" )
SET( DCACHE_GUI_INCLUDE_DIRS "${DCACHE_GUI_INCLUDE_DIRS}")

SET( DCACHE_GUI_LIBRARIES "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( DCACHE_GUI_DEFINITIONS @CALICE_USERLIB_EXPORT_DEFINITIONS@ )

SET( DCACHE_GUI_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( DCACHE_GUI_MAJOR_VERSION "@DCACHE_GUI_MAJOR_VERSION@" )
SET( DCACHE_GUI_MINOR_VERSION "@DCACHE_GUI_MINOR_VERSION@" )
SET( DCACHE_GUI_PATCH_LEVEL    "@DCACHE_GUI_PATCH_LEVEL@"    )


SET( DCACHE_GUI_FOUND TRUE )